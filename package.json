{
	"name": "create-font-from-fontello",
	"version": "0.0.0",
	"private": true,
	"description": "",
	"repository": {
		"type": "git",
		"url": ""
	},
	"type": "module",
	"bin": "./dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm --clean --minify --metafile",
		"changeset": "changeset",
		"dev-": "tsup src/index.ts --format esm --watch --clean --onSuccess \"node dist/index.js\"",
		"dev": "tsup src/index.ts --format esm --watch --clean",
		"prepare": "husky install",
		"release": "changeset publish",
		"test": "vitest"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{md,mdx,yml,json}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@types/ejs": "^3.1.5",
		"@types/svg2ttf": "^5.0.3",
		"axios": "^1.6.8",
		"chalk": "^5.3.0",
		"commander": "^9.5.0",
		"ejs": "^3.1.10",
		"figlet": "^1.5.2",
		"lodash": "^4.17.21",
		"ora": "^6.1.2",
		"svg2ttf": "^6.0.3",
		"svgpath": "^2.6.0",
		"uuid": "^9.0.1",
		"yup": "^1.4.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.0",
		"@commitlint/cli": "^19.3.0",

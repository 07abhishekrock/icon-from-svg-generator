		"@types/figlet": "^1.5.5",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"@xmldom/xmldom": "^0.8.10",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.2",
		"tsup": "^6.5.0",
		"type-fest": "^3.5.1",
		"typescript": "^4.9.4",
		"vitest": "^0.27.1"
	},
	"engines": {
		"node": ">=14.16"
	}
